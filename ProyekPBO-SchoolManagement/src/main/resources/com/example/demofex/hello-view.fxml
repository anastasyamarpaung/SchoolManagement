<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableRow?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      stylesheets="@css/style.css"
      fx:controller="com.example.demofex.Controller.Home">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <GridPane styleClass="Home" vgap="20" hgap="20" alignment="CENTER">
        <VBox styleClass="GroupeTitle" GridPane.rowIndex="0">
            <Text styleClass="BigTitle">ASCMS</Text>
            <Text styleClass="SubTitle">Apocalypse School Management System</Text>
        </VBox>
        <VBox alignment="CENTER" GridPane.rowIndex="1">
            <Text>
                Apa yang ingin anda lakukan hari ini?
            </Text>
        </VBox>
        <GridPane GridPane.rowIndex="2" hgap="10">
            <Button GridPane.columnIndex="0" onMouseClicked="#opentmpage">Mengatur data Guru</Button>
            <Button GridPane.columnIndex="1" onMouseClicked="#opencmpage">Mengatur data Kelas</Button>
            <Button GridPane.columnIndex="2" onMouseClicked="#openstmpage">Mengatur Informasi Murid</Button>
            <Button GridPane.columnIndex="3" onMouseClicked="#opensmpage">Mengatur Informasi Staff</Button>
        </GridPane>
    </GridPane>
</VBox>
